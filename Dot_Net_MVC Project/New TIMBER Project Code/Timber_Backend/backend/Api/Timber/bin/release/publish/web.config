<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <location path="." inheritInChildApplications="false">
    <system.webServer>
      <handlers>
        <add name="aspNetCore" path="*" verb="*" modules="AspNetCoreModuleV2" resourceType="Unspecified" />
      </handlers>
      <aspNetCore processPath="dotnet" arguments=".\Bill.dll" stdoutLogEnabled="false" stdoutLogFile=".\logs\stdout" hostingModel="inprocess" />
    </system.webServer>
    <system.webServer>
        <rewrite>
            <rules>
                <rule name="ReverseProxyInbound">
                    <match url=".*" />
                    <action type="Rewrite" url="http://localhost:5200/{R:0}" />
                </rule>
            </rules>
        </rewrite>
    </system.webServer>
  </location>
</configuration>
